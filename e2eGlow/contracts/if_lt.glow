#lang glow

@interaction([A, B])
let fewif = (parA, parB) => {
    let handA = parA; //5  50
    deposit! A -> handA;

    let handB = parB; //5 50
    deposit! B -> handB;

    if(parA < 10){
       withdraw! A <- handB
    }else{
       withdraw! B <- handA //200
    };

    if(parB < 20){
       withdraw! B <- handA
    }else{
       withdraw! B <- handB //250
    }


};