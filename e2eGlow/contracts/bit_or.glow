#lang glow

@interaction([A, B])

let bitOr = (parA, parB) => {

    let handA = parA; //10
    deposit! A -> handA;

    let handB = parB; //20
    deposit! B -> handB;

    let bitwiseOr = (handA ||| handB);
    if(bitwiseOr == 30){
         withdraw! A <- handA + handB
    }else{
         withdraw! B <- handA + handB
    }
};